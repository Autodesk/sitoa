[node wireframe]
soft.category STRING "Utility"
soft.order STRING "edge_type fill_color line_color line_width raster_space"

[attr edge_type]
desc STRING "Determines how the mesh is represented. Quads will render as polygon faces. If triangles is selected, polygons will be broken up into their triangular tessellation"

[attr line_width]
desc STRING "The thickness of the edge lines of the polygon faces in pixel units."
min     FLOAT 0
softmin FLOAT 0
softmax FLOAT 3

[attr fill_color]
desc STRING "The color used to represent the polygon faces."

[attr line_color]
desc STRING "The color used to represent the edges."

[attr raster_space]
desc STRING "By enabling Raster Space, the shader will interpret the Line Width parameter as a number of pixels, otherwise Line Width is measured in world space."
