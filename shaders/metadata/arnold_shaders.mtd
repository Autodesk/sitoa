[node standard_hair]
soft.category STRING "Surface"
soft.order STRING "BeginGroup Color base base_color melanin melanin_redness melanin_randomize EndGroup "
"BeginGroup Specular roughness ior shift EndGroup "
"BeginGroup Tint specular_tint specular2_tint transmission_tint EndGroup "
"BeginGroup Diffuse diffuse diffuse_color EndGroup "
"BeginGroup Emission emission emission_color EndGroup "
"BeginGroup Advanced opacity indirect_diffuse indirect_specular extra_depth EndGroup"

[attr base]
desc STRING "Brightness of the hair, a multiplier for the base color."
soft.label STRING "Weight"
min FLOAT 0
max FLOAT 1

[attr base_color]
desc STRING "The color of the hair. This controls absorption inside hair fibers, which gives hair its color as light scatters around."
soft.label STRING "Color"
soft.viewport_guid STRING "{3515CC72-082C-11D0-91DE-00A024C78EE3}"

[attr melanin]
desc STRING "Amount of melanin in hair. Colors will range from blonde around 0.2, "
"to red and brown around 0.5, to black at 1.0."
min FLOAT 0
max FLOAT 1

[attr melanin_redness]
desc STRING "Controls the redness of hair. Higher values increase the proportion of red pheomelanin."
min FLOAT 0
max FLOAT 1

[attr melanin_randomize]
desc STRING "Randomizes the amount of melanin in hair fibers, for variation in hair colors (0.. 1)."
min FLOAT 0
max FLOAT 1

[attr roughness]
desc STRING "Roughness of hair specular reflections and transmission."
min FLOAT 0
max FLOAT 1

[attr ior]
desc STRING "Index of refraction. Lower IOR values give stronger forward scattering, "
"and higher values give stronger reflection."
min FLOAT 1
softmin FLOAT 0
softmax FLOAT 3

[attr shift]
desc STRING "Angle of scales on the hair fiber, shifting the primary and secondary specular "
"reflections away from the perfect mirror direction."
min FLOAT -90
max FLOAT 90
softmin FLOAT 0
softmax FLOAT 20

[attr specular_tint]
desc STRING "Scale of the primary specular contribution."
soft.label STRING "Specular 1"
soft.viewport_guid STRING "{3515CC73-082C-11D0-91DE-00A024C78EE3}"

[attr specular2_tint]
desc STRING "Scale of the secondary specular contribution"
soft.label STRING "Specular 2"

[attr transmission_tint]
desc STRING "Transmission Tint."
soft.label STRING "Transmission"

[attr diffuse]
desc STRING "Diffuseness of hair, with 0 giving fully specular scattering, and 1 fully diffuse scattering."
soft.label STRING "Weight"
min FLOAT 0
max FLOAT 1

[attr diffuse_color]
desc STRING "Diffuse scattering color."
soft.label STRING "Color"

[attr emission]
desc STRING "Multiplier for the emission color."
soft.label STRING "Weight"
min FLOAT 0
softmin FLOAT 0
softmax FLOAT 1

[attr emission_color]
desc STRING "Color used as emission."
soft.label STRING "Color"

[attr opacity]
desc STRING "Opacity of the hair."

[attr indirect_diffuse]
desc STRING "The amount of diffuse light received from indirect sources only."
min FLOAT 0
max FLOAT 1

[attr indirect_specular]
desc STRING "The amount of specularity received from indirect sources only."
min FLOAT 0
max FLOAT 1

[attr extra_depth]
desc STRING "Adds extra Specular Ray Depth just for this shader."
min INT 0
max INT 64

